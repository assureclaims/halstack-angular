    "@scullyio/ng-lib": "^2.1.41",
    "@scullyio/scully": "^2.1.41",
    "@types/jest": "^29.5.1",
    "amazon-cognito-identity-js": "^6.2.0",
    "aws-sdk": "^2.1366.0",
    "babel-jest": "^29.5.0",
