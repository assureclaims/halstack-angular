{
  "name": "@dxc-technology/halstack-angular",
  "version": "5.1.0",
  "description": "DXC Angular CDK (Component Development Kit) based on Graphen Design System",
  "scripts": {
    "build-lib": "ng build dxc-ngx-cdk",
    "build-lib-prod": "ng build --prod dxc-ngx-cdk",
    "package": "cd ./../../dist/dxc-ngx-cdk && npm pack",
    "post-build-lib": "cp -R ./src/assets ./../../dist/dxc-ngx-cdk/lib/assets && npm run copy-styles-lib && npm run copy-md-lib",
    "copy-styles-lib": "cp -R ./src/lib/styles ./../../dist/dxc-ngx-cdk/lib/styles ",
    "copy-md-lib": "cp ./../../README.md ./../../dist/dxc-ngx-cdk/",
    "post-build-lib-win": "xcopy .\\src\\assets .\\..\\..\\dist\\dxc-ngx-cdk\\lib\\assets /Y /I && npm run copy-styles-lib-win",
    "copy-styles-lib-win": "xcopy .\\src\\lib\\styles .\\..\\..\\dist\\dxc-ngx-cdk\\lib\\styles /Y /I /E",
    "generate-lib": "npm run build-lib && npm run post-build-lib && npm run package",
    "generate-lib-prod": "npm run build-lib-prod && npm run post-build-lib && npm run package",
    "generate-lib-win": "npm run build-lib && npm run post-build-lib-win && npm run package",
    "generate-lib-prod-win": "npm run build-lib-prod && npm run post-build-lib-win && npm run package",
    "test": "jest --config ./jest.lib.config.js",
    "build-lib:watch": "ng build --watch dxc-ngx-cdk"
  },
  "author": {
    "name": "DXC Technology",
    "email": "DigitalInsurance@dxc.com",
    "url": "https://dxc.com"
  },
