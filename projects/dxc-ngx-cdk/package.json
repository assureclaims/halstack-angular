{
  "name": "@dxc-technology/halstack-angular",
  "version": "0.0.0",
  "description": "DXC Angular CDK (Component Development Kit) based on Graphen Design System",
  "resolutions": {
    "webpack": "^5.0.0"
  },
  "scripts": {
    "build-lib": "ng build dxc-ngx-cdk",
    "build-lib-prod": "ng build --prod dxc-ngx-cdk",
    "package": "cd ./../../dist/dxc-ngx-cdk && npm pack",
    "post-build-lib": "cp -R ./src/assets ./../../dist/dxc-ngx-cdk/lib/assets && npm run copy-styles-lib && npm run copy-md-lib",
    "copy-styles-lib": "cp -R ./src/lib/styles ./../../dist/dxc-ngx-cdk/lib/styles ",
    "copy-md-lib": "cp ./../../README.md ./../../dist/dxc-ngx-cdk/",
    "post-build-lib-win": "xcopy .\\src\\assets .\\..\\..\\dist\\dxc-ngx-cdk\\lib\\assets /Y /I && npm run copy-styles-lib-win",
    "copy-styles-lib-win": "xcopy .\\src\\lib\\styles .\\..\\..\\dist\\dxc-ngx-cdk\\lib\\styles /Y /I /E",
    "generate-lib": "npm run build-lib && npm run post-build-lib && npm run package",
    "generate-lib-prod": "npm run build-lib-prod && npm run post-build-lib && npm run package",
    "generate-lib-win": "npm run build-lib && npm run post-build-lib-win && npm run package",
    "generate-lib-prod-win": "npm run build-lib-prod && npm run post-build-lib-win && npm run package",
    "test": "jest --config ./jest.lib.config.js",
    "build-lib:watch": "ng build --watch dxc-ngx-cdk",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": {
    "name": "DXC Technology",
    "email": "DigitalInsurance@dxc.com",
    "url": "https://dxc.com"
  },
  "dependencies": {
    "@angular/animations": "12.2.16",
    "@angular/cdk": "12.2.13",
    "@angular/common": "12.2.16",
    "@angular/compiler": "12.2.16",
    "@angular/core": "12.2.16",
    "@angular/forms": "12.2.16",
    "@angular/material": "12.2.13",
    "@angular/material-moment-adapter": "12.2.13",
    "@angular/platform-browser": "12.2.16",
    "@angular/platform-browser-dynamic": "12.2.16",
    "@angular/router": "12.2.16",
    "@ctrl/tinycolor": "^3.4.0",
    "@testing-library/angular": "^10.1.2",
    "color": "^3.1.3",
    "emotion": "10.0.27",
    "jest-preset-angular": "^7.1.1",
    "moment": "^2.24.0",
    "ng-mocks": "^10.4.0",
    "rgb-hex": "^3.0.0",
    "rxjs": "~6.5.2",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2",
    "webpack": "^5.69.1",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.16",
    "@angular-devkit/build-ng-packagr": "0.1002.0",
    "@angular/cli": "12.2.16",
    "@angular/compiler-cli": "12.2.16",
    "@angular/language-service": "12.2.16",
    "@angular/localize": "12.2.16",
    "@babel/core": "^7.4.5",
    "@testing-library/dom": "^8.6.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/user-event": "^13.2.1",
    "babel-loader": "^8.0.6",
    "browserslist": "^4.19.3",
    "chromatic": "^6.3.3",
    "codelyzer": "^6.0.0",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "jest": "^24.9.0",
    "karma": "4.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.1",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "12.2.6",
    "postcss-pseudo-classes": "^0.3.0",
    "protractor": "~7.0.0",
    "rgb-hex": "^3.0.0",
    "start-server-and-test": "^1.10.6",
    "ts-node": "7.0.0",
    "tsickle": "^0.37.1",
    "typescript": "4.3.5"
  }
}
